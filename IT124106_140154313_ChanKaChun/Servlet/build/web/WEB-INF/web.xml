<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>EducationServlet</servlet-name>
        <servlet-class>EducationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgeServlet</servlet-name>
        <servlet-class>AgeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DistrictServlet</servlet-name>
        <servlet-class>DistrictServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HttpXmlClient</servlet-name>
        <servlet-class>HttpXmlClient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EduServletJson</servlet-name>
        <servlet-class>EduServletJson</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EducationServlet</servlet-name>
        <url-pattern>/EducationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgeServlet</servlet-name>
        <url-pattern>/AgeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DistrictServlet</servlet-name>
        <url-pattern>/DistrictServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HttpXmlClient</servlet-name>
        <url-pattern>/HttpXmlClient</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EduServletJson</servlet-name>
        <url-pattern>/EduServletJson</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
